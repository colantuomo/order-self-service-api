generator client {
  provider      = "prisma-client-js"
  binaryTargets = ["native", "linux-musl-openssl-3.0.x"]
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model users {
  id   Int    @id @default(autoincrement())
  name String @db.VarChar(255)
  cpf  String @unique @db.VarChar(11)
}
